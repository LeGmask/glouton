{
  "name": "glouton-panel",
  "version": "0.0.1",
  "description": "UI for local Glouton",
  "browserslist": [
    "defaults",
    "last 5 year",
    "firefox 45"
  ],
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "npm run clean && webpack build --mode production && rm dist/*.svg",
    "deploy": "npm run build && npm run cleanOldJsFiles && mkdir -p ../api/static/assets/js && mkdir -p ../api/static/assets/css && for f in $(find ./dist/js -name '*.js'); do cat $f | gzip > ../api/static/assets/js/$(basename $f); done && cp -r ./dist/css/ ../api/static/assets/",
    "cleanOldJsFiles": "rm -rf ../api/static/assets/js && rm -rf ../api/static/assets/css",
    "clean": "rm -rf dist",
    "lint": "eslint src",
    "prettify": "prettier --write 'src/'"
  },
  "author": "Tom Darneix",
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.8.3",
    "apollo-link-logger": "^2.0.1",
    "axios": "^1.5.0",
    "bootstrap": "^5.3.2",
    "classnames": "^2.3.2",
    "core-js": "^3.32.2",
    "d3": "^7.8.5",
    "echarts": "^5.4.3",
    "express": "^4.18.2",
    "graphql": "^16.8.0",
    "history": "^5.3.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "rc-switch": "^4.1.0",
    "react": "^18.2.0",
    "react-bootstrap-table-next": "^4.0.3",
    "react-bootstrap-table2-paginator": "^2.1.2",
    "react-c3js": "^0.1.20",
    "react-collapse": "^5.1.1",
    "react-day-picker": "^8.8.2",
    "react-debounce-input": "^3.3.0",
    "react-dom": "^18.2.0",
    "react-motion": "^0.5.2",
    "react-query": "^3.39.3",
    "react-router": "^6.16.0",
    "react-router-dom": "^6.16.0",
    "react-tooltip": "^5.21.4",
    "react-visibility-sensor": "^5.1.1",
    "reactstrap": "^9.2.0",
    "serialize-javascript": "^6.0.1",
    "tabler-react": "^1.30.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.20",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-lodash": "^3.3.4",
    "babel-regenerator-runtime": "^6.5.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "mini-css-extract-plugin": "^2.7.6",
    "prettier": "^3.0.3",
    "sass": "^1.67.0",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
