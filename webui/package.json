{
  "name": "glouton-panel",
  "version": "0.0.1",
  "description": "UI for local Glouton",
  "browserslist": [
    "defaults",
    "last 5 year",
    "firefox 45"
  ],
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "npm run clean && webpack build --mode production && rm dist/*.svg",
    "deploy": "npm run build && npm run cleanOldJsFiles && mkdir -p ../api/static/assets/js && mkdir -p ../api/static/assets/css && for f in $(find ./dist/js -name '*.js'); do cat $f | gzip > ../api/static/assets/js/$(basename $f); done && cp -r ./dist/css/ ../api/static/assets/",
    "cleanOldJsFiles": "rm -rf ../api/static/assets/js && rm -rf ../api/static/assets/css",
    "clean": "rm -rf dist",
    "lint": "eslint src",
    "pretify": "prettier --write 'src/'"
  },
  "author": "Tom Darneix",
  "license": "ISC",
  "dependencies": {
    "@apollo/react-hooks": "^4.0.0",
    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-http": "^1.5.17",
    "apollo-link-logger": "^1.2.3",
    "axios": "^0.24.0",
    "bootstrap": "^5.1.3",
    "classnames": "^2.3.1",
    "core-js": "^3.20.2",
    "d3": "^7.2.1",
    "echarts": "^5.2.2",
    "express": "^4.17.2",
    "graphql": "^15.8.0",
    "history": "^5.2.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "rc-switch": "^3.2.2",
    "react": "^17.0.2",
    "react-bootstrap-table-next": "^4.0.3",
    "react-bootstrap-table2-paginator": "^2.1.2",
    "react-c3js": "^0.1.20",
    "react-collapse": "^5.1.1",
    "react-day-picker": "^7.4.10",
    "react-debounce-input": "^3.2.5",
    "react-dom": "^17.0.2",
    "react-motion": "^0.5.2",
    "react-query": "^3.34.7",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "react-tooltip": "^4.2.21",
    "react-visibility-sensor": "^5.1.1",
    "reactstrap": "^9.0.1",
    "serialize-javascript": "^6.0.0",
    "tabler-react": "^1.30.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-lodash": "^3.3.4",
    "babel-regenerator-runtime": "^6.5.0",
    "css-loader": "^6.5.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "mini-css-extract-plugin": "^2.4.5",
    "prettier": "^2.5.1",
    "sass": "^1.45.2",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.2"
  }
}
