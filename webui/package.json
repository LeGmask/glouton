{
  "name": "glouton-panel",
  "version": "0.0.1",
  "description": "UI for local Glouton",
  "browserslist": [
    "defaults",
    "last 5 year",
    "firefox 45"
  ],
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "npm run clean && webpack build --mode production && rm dist/*.svg",
    "deploy": "npm run build && npm run cleanOldJsFiles && mkdir -p ../api/static/assets/js && mkdir -p ../api/static/assets/css && for f in $(find ./dist/js -name '*.js'); do cat $f | gzip > ../api/static/assets/js/$(basename $f); done && cp -r ./dist/css/ ../api/static/assets/",
    "cleanOldJsFiles": "rm -rf ../api/static/assets/js && rm -rf ../api/static/assets/css",
    "clean": "rm -rf dist",
    "lint": "eslint src",
    "pretify": "prettier --write 'src/'"
  },
  "author": "Tom Darneix",
  "license": "ISC",
  "dependencies": {
    "@apollo/react-hooks": "^4.0.0",
    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-http": "^1.5.17",
    "apollo-link-logger": "^2.0.0",
    "axios": "^0.21.3",
    "bootstrap": "^5.1.0",
    "classnames": "^2.3.1",
    "core-js": "^3.17.2",
    "d3": "^7.0.1",
    "echarts": "^5.2.0",
    "express": "^4.17.1",
    "graphql": "^15.5.3",
    "history": "^5.0.1",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "rc-switch": "^3.2.2",
    "react": "^17.0.2",
    "react-bootstrap-table-next": "^4.0.3",
    "react-bootstrap-table2-paginator": "^2.1.2",
    "react-c3js": "^0.1.20",
    "react-collapse": "^5.1.0",
    "react-day-picker": "^7.4.10",
    "react-debounce-input": "^3.2.5",
    "react-dom": "^17.0.2",
    "react-motion": "^0.5.2",
    "react-query": "^3.21.1",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "react-tooltip": "^4.2.21",
    "react-visibility-sensor": "^5.1.1",
    "reactstrap": "^8.10.0",
    "serialize-javascript": "^6.0.0",
    "tabler-react": "^1.30.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-function-bind": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-react": "^7.14.5",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-lodash": "^3.3.4",
    "babel-regenerator-runtime": "^6.5.0",
    "css-loader": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
