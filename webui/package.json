{
  "name": "glouton-panel",
  "version": "0.0.1",
  "description": "UI for local Glouton",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.dev.js",
    "build": "npm run clean && webpack -p --config webpack.config.production.js --bail",
    "deploy": "npm run build && npm run cleanOldJsFiles && mkdir -p ../api/static/assets/js && for f in $(find ./dist/js -name '*.js'); do cat $f | gzip > ../api/static/assets/js/$(basename $f); done",
    "cleanOldJsFiles": "rm -rf ../api/static/assets/js",
    "clean": "rm -rf dist",
    "lint": "eslint src",
    "flow": "flow",
    "pretify": "prettier --write 'src/**/*.js'"
  },
  "author": "Tom Darneix",
  "license": "ISC",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-http": "^1.5.17",
    "apollo-link-logger": "^1.2.3",
    "bootstrap": "^4.5.3",
    "classnames": "^2.2.6",
    "core-js": "^3.7.0",
    "echarts": "^4.9.0",
    "express": "^4.17.1",
    "graphql": "^14.7.0",
    "history": "^4.7.2",
    "lodash": "^4.17.20",
    "prop-types": "^15.6.2",
    "rc-switch": "^1.9.2",
    "react": "^16.14.0",
    "react-bootstrap-table-next": "^4.0.3",
    "react-bootstrap-table2-paginator": "^2.0.1",
    "react-c3js": "^0.1.20",
    "react-collapse": "^5.1.0",
    "react-day-picker": "^7.4.8",
    "react-debounce-input": "^3.2.3",
    "react-dom": "^16.14.0",
    "react-motion": "^0.5.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-tooltip": "^4.2.10",
    "react-visibility-sensor": "^5.0.2",
    "reactstrap": "^8.7.1",
    "serialize-javascript": "^4.0.0",
    "tabler-react": "^1.18.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-function-bind": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-flow": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.2.1",
    "babel-plugin-lodash": "^3.3.4",
    "babel-regenerator-runtime": "^6.5.0",
    "css-loader": "^3.6.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.0.1",
