{
  "name": "glouton-panel",
  "version": "0.0.1",
  "description": "UI for local Glouton",
  "browserslist": [
    "defaults",
    "last 5 year",
    "firefox 45"
  ],
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "npm run clean && webpack build --mode production && rm dist/*.svg",
    "deploy": "npm run build && npm run cleanOldJsFiles && mkdir -p ../api/static/assets/js && mkdir -p ../api/static/assets/css && for f in $(find ./dist/js -name '*.js'); do cat $f | gzip > ../api/static/assets/js/$(basename $f); done && cp -r ./dist/css/ ../api/static/assets/",
    "cleanOldJsFiles": "rm -rf ../api/static/assets/js && rm -rf ../api/static/assets/css",
    "clean": "rm -rf dist",
    "lint": "eslint src",
    "prettify": "prettier --write 'src/'"
  },
  "author": "Tom Darneix",
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.7.11",
    "apollo-link-logger": "^2.0.1",
    "axios": "^1.3.5",
    "bootstrap": "^5.2.3",
    "classnames": "^2.3.2",
    "core-js": "^3.30.0",
    "d3": "^7.8.4",
    "echarts": "^5.4.2",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "history": "^5.3.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "rc-switch": "^4.0.0",
    "react": "^18.2.0",
    "react-bootstrap-table-next": "^4.0.3",
    "react-bootstrap-table2-paginator": "^2.1.2",
    "react-c3js": "^0.1.20",
    "react-collapse": "^5.1.1",
    "react-day-picker": "^8.7.0",
    "react-debounce-input": "^3.3.0",
    "react-dom": "^18.2.0",
    "react-motion": "^0.5.2",
    "react-query": "^3.39.3",
    "react-router": "^6.10.0",
    "react-router-dom": "^6.10.0",
    "react-tooltip": "^5.11.1",
    "react-visibility-sensor": "^5.1.1",
    "reactstrap": "^9.1.9",
    "serialize-javascript": "^6.0.1",
    "tabler-react": "^1.30.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-lodash": "^3.3.4",
    "babel-regenerator-runtime": "^6.5.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "mini-css-extract-plugin": "^2.7.5",
    "prettier": "^2.8.7",
    "sass": "^1.62.0",
    "sass-loader": "^13.2.2",
    "style-loader": "^3.3.2",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "^5.78.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.2"
  }
}
